{
    "version": 2,
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "flask",
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ]
}